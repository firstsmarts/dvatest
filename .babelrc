{
    "presets": [
        "es2015",
        "react"
    ],
    "plugins": [
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "lib",
                "style": "css"
            }
        ],
        "syntax-dynamic-import",
        "syntax-decorators",
        "transform-object-rest-spread",
        "transform-regenerator",
        "syntax-async-functions",
        ["transform-runtime",{
            "polyfill": false,
            "regenerator": true
        }]
    ]
}